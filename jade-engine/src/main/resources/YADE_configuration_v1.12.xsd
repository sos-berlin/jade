			      <xs:attribute name="ref" type="xs:NMTOKEN" use="required">
            <xs:annotation>
               <xs:documentation>
                  <div xmlns="http://www.w3.org/1999/xhtml">
                     <p>
			This attribute identifies the fragment that is used for inserting secure configuration items.
		</p>
                  </div>
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
