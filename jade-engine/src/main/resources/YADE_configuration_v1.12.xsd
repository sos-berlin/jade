				        <FlatParameter name="strict_host_key_checking"/>
				        <FlatParameter name="StrictHostKeyChecking"/>
			      </xs:appinfo>
         <xs:documentation source="https://kb.sos-berlin.com/display/PKB/YADE+Parameter+Reference+-+StrictHostkeyChecking">
