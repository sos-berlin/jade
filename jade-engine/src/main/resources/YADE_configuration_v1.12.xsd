    <xs:element name="ChannelConnectTimeout" default="0s">
		    <xs:annotation>
         <xs:appinfo>
				        <FlatParameter name="channel_connect_timeout"/>
				        <Release starting="1.12.13, 1.13.4" issue="YADE-555"/>
